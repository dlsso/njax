{
    "app_name":"NJax",
    "frameworks":{
    },
    "models":{
        "account":{
            "fields":{
                "email":"email",
                "name":"string",
                "namespace":"namespace"
            }
        },
        "application":{
            "fields":{
                "username":"namespace",
                "name":"string",
                "desc":"md",
                "url":"string",
                "secret":"string",
                "level":"level",
                "owner":{
                    "type":"ref",
                    "ref":"account"
                }
            }
        },
        "accessToken":{
            "fields":{
                "perms":"array",
                "token":"string",
                "application":{
                    "type":"ref",
                    "ref":"application"
                },
                "account":{
                    "type":"ref",
                    "ref":"account"
                }
            }
        },
        "requestCode":{
            "fields":{
                "code":"string",
                "application":{
                    "type":"ref",
                    "ref":"application"
                }
            }
        }
    },
    "bowser":{
        "dependencies":{
            "fontawesome":"*"
        }
    },
    "package":{
        "name": "njax",
        "version": "0.0.1",
        "private": false,
        "scripts": {
            "start": "node index.js"
        },
        "dependencies": {
            "express": "4.*",
            "hjs": "git+ssh://git@github.com:schematical/hjs.git",
            "underscore": "*",
            "async": "*",
            "request": "*",
            "hogan":"*",
            "mkdirp": "*",
            "markdown": "*",
            "aws-sdk":"*",
            "serve-static":"*",
            "mongoose": "3.8.5",
            "oauth2orize": "0.x.x",
            "passport": "~0.1.12",
            "passport-local": "0.1.x",
            "passport-local-mongoose": "*",
            "passport-http": "0.2.x",
            "passport-http-bearer": "0.2.x",
            "passport-oauth2-client-password": "0.1.x",
            "serve-static":"*",
            "morgan": "~1.0.0",
            "body-parser": "~1.0.0",
            "cookie-parser":"*",
            "express-session":"*",
            "errorhandler":"*"
        }
    }
}